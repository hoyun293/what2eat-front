{
  "name": "what2eat-front",
  "description": "What 2 Eat Frontend App",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "jetifier",
    "dev": "concurrently \"npm run server\" \"npm run front\"",
    "front": "ionic serve -p 5500",
    "server": "nodemon --watch server server/index.js",
    "build": "react-scripts build",
    "serve:build": "npx serve -s build",
    "sync": "yarn build && npx cap sync",
    "dev:ios": "ionic capacitor run ios -l --external",
    "dev:and": "ionic capacitor run android -l --external",
    "open:and": "npx cap open android",
    "open:ios": "npx cap open ios",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@capacitor/android": "^2.3.0",
    "@capacitor/core": "^2.3.0",
    "@capacitor/ios": "^2.3.0",
    "@ionic-native/branch-io": "^5.23.0",
    "@ionic-native/core": "^5.23.0",
    "@ionic/react": "^4.11.10",
    "@ionic/react-router": "^4.11.10",
    "@types/react-redux": "^7.1.7",
    "@types/react-slick": "^0.23.4",
    "@types/react-swipe": "^6.0.0",
    "@types/redux-actions": "^2.6.1",
    "axios": "^0.19.2",
    "branch-cordova-sdk": "^4.1.3",
    "copy-to-clipboard": "^3.3.1",
    "google-map-react": "^1.1.7",
    "ionicons": "^4.6.3",
    "jetifier": "^1.6.6",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-scroll-horizontal": "^1.6.4",
    "react-slick": "^0.25.2",
    "react-swipe": "^6.0.4",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "reselect": "^4.0.0",
    "slick-carousel": "^1.8.1",
    "swipe-js-iso": "^2.1.5"
  },
  "devDependencies": {
    "@capacitor/cli": "2.3.0",
    "@types/google-map-react": "^1.1.5",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.31",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/recompose": "^0.30.7",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dts-gen": "^0.6.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "morgan": "^1.10.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
